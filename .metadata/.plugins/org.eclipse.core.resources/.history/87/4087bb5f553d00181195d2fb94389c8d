package gameController;

public class TurnChecker {
	private static int count;
	private TurnChecker turnChecker;

	public TurnChecker(){

	}

	public TurnChecker getTurnChecker(){
		if(turnChecker == null){
			turnChecker = new TurnChecker();
		}

		return turnChecker;
	}

	public int getCount(){
		return count;
	}
}
