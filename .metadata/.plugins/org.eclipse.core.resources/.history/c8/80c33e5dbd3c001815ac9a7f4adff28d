package gameModel;

public abstract class Hero implements Move{
	private PlayerType pType;
	private RoleType rType;
	private int startX;
	private int startY;

	public Hero(){

	}

	public PlayerType getPlayerType(){
		return pType;
	}

	public RoleType getRoleType(){
		return rType;
	}

	public int getStartX(){
		return startX;
	}

	public int getStartY(){
		return startY;
	}
}
