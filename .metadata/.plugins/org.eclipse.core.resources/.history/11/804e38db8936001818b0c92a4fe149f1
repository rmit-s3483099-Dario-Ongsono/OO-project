package controller;

import model.Role;
import model.Tile;

public class MoveDetector {
	private Role heroSelected;
	private Tile[][] tile;

	public void MoveDetector(Role hero, Tile[][] tile){
		this.heroSelected = hero;
		this.tile = tile;
	}

	public void validTile(){
		if(tile[heroSelected.getInitX()][heroSelected.getInitY()].hasHero())
	}


}
