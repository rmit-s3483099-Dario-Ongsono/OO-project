package test;

public class Test {

	public static void main(String[] args) {
		
		
		int[][] a = new int[3][3];
		int c= 5;
		int balaTest = 9;
		int balance = 0;
		
		while(balaTest > 8 && balance < 8){
			for(int x = 0; x < 3; x++){
				for(int y = 0; y < 3; y++){
					if(a[x][y] > c){
						balance -= 1;
						break;
					}
						
					else
						balance += 1;
				}
			}
		}
	}

}
